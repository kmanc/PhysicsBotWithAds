    <string name="vmiasolvevo">
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x - x₀ - ½aΔt² = V₀Δt \n\n
        (x - x₀ - ½aΔt²) / Δt = V₀
    </string>
    <string name="vmiasolvet">
        x = x₀ + V₀Δt + ½aΔt² \n\n
        0 = x₀ - x + VoΔt + ½aΔt² \n\n
        Use the quadratic equation where \n
        A = ½a, B = V₀, C = x₀-x \n\n
        Δt = [V₀ ± sqrt(V₀² - 2a(x₀ - x))] / a
    </string>
    <string name="vmiasolvexo">
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x₀ = x - V₀Δt - ½aΔt²
    </string>
    <string name="vmiasolvex">
        x = x₀ + V₀Δt + ½aΔt²
    </string>
    <string name="vmiasolvea">
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x - x₀ - V₀Δt = ½aΔt²\n\n
        2(x - x₀ - V₀Δt) / Δt² = V₀
    </string>
    <string name="vomiasolvev">
        V = V₀ + aΔt \n\n
        V₀ = V - aΔt \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + (V - aΔt)Δt + ½aΔt² \n\n
        x = x₀ + VΔt - ½aΔt² \n\n
        x - x₀ + ½aΔt² = VΔt \n\n
        (x - x₀ + ½aΔt²) / Δt = V
    </string>
    <string name="vomiasolvet">
        V = V₀ + aΔt \n\n
        V₀ = V - aΔt \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + (V - aΔt)Δt + ½aΔt² \n\n
        x = x₀ + VΔt - ½aΔt² \n\n
        ½aΔt² - VΔt + x - x₀
        Use the quadratic equation where \n
        A = ½a, B = V, C = x - x₀ \n\n
        Δt = [V ± sqrt(V² - 2a(x - x₀))] / a
    </string>
    <string name="vomiasolvexo">
        V = V₀ + aΔt \n\n
        V₀ = V - aΔt \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + (V - aΔt)Δt + ½aΔt² \n\n
        x = x₀ + VΔt - ½aΔt² \n\n
        x₀ = x - VΔt + ½aΔt²
    </string>
    <string name="vomiasolvex">
        V = V₀ + aΔt \n\n
        V₀ = V - aΔt \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + (V - aΔt)Δt + ½aΔt² \n\n
        x = x₀ + VΔt - ½aΔt²
    </string>
    <string name="vomiasolvea">
        V = V₀ + aΔt \n\n
        V₀ = V - aΔt \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + (V - aΔt)Δt + ½aΔt² \n\n
        x = x₀ + VΔt - ½aΔt² \n\n
        x - x₀ - VΔt = ½aΔt² \n\n
        2(VΔt + x₀ - x) / Δt² = a
    </string>
    <string name="tmiasolvev">
        V² = V₀² + 2a(x - x₀) \n\n
        V = sqrt(V₀² + 2a(x - x₀))
    </string>
    <string name="tmiasolvevo">
        V² = V₀² + 2a(x - x₀) \n\n
        V² - 2a(x - x₀) = V₀² \n\n
        sqrt(V² - 2a(x - x₀)) = V₀
    </string>
    <string name="tmiasolvexo">
        V² = V₀² + 2a(x - x₀) \n\n
        V² - V₀² = 2a(x - x₀) \n\n
        (V² - V₀²) / 2a = (x - x₀) \n\n
        [(V² - V₀²) / 2a] - x = -x₀ \n\n
        x - [(V² - V₀²) / 2a] = x₀
    </string>
    <string name="tmiasolvex">
        V² = V₀² + 2a(x - x₀) \n\n
        V² - V₀² = 2a(x - x₀) \n\n
        (V² - V₀²) / 2a = (x - x₀) \n\n
        [(V² - V₀²) / 2a] + x₀ = x
    </string>
    <string name="tmiasolvea">
        V² = V₀² + 2a(x - x₀) \n\n
        V² - V₀² = 2a(x - x₀) \n\n
        (V² - V₀²) / 2(x - x₀) = a
    </string>
