    <string name="amiasolvev">
        V = V₀ + aΔt \n\n
        V - V₀ = aΔt \n\n
        (V - V₀) / Δt = a \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + V₀Δt + ½(V - V₀)Δt² / Δt \n\n
        x = x₀ + V₀Δt + ½(V - V₀)Δt \n\n
        x - x₀ - V₀Δt = ½(V - V₀)Δt \n\n
        2(x - x₀ - V₀Δt) / Δt = V - V₀ \n\n
        [2(x - x₀ - V₀Δt) / Δt] + V₀ = V</string>
    <string name="amiasolvevo">
        V = V₀ + aΔt \n\n
        V - V₀ = aΔt \n\n
        (V - V₀) / Δt = a \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + V₀Δt + ½(V - V₀)Δt² / Δt \n\n
        x = x₀ + V₀Δt + ½(V - V₀)Δt \n\n
        x - x₀ - V₀Δt = ½(V - V₀)Δt \n\n
        2(x - x₀ - V₀Δt) / Δt = V - V₀ \n\n
        [2(x - x₀ - V₀Δt) / Δt] - V = - V₀ \n\n
        V - [2(x - x₀ - V₀Δt) / Δt] = V₀
    </string>
    <string name="amiasolvexo">
        V = V₀ + aΔt \n\n
        V - V₀ = aΔt \n\n
        (V - V₀) / Δt = a \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + V₀Δt + ½(V - V₀)Δt² / Δt \n\n
        x = x₀ + V₀Δt + ½(V - V₀)Δt \n\n
        x - V₀Δt - ½(V - V₀)Δt = x₀
    </string>
    <string name="amiasolvex">
        V = V₀ + aΔt \n\n
        V - V₀ = aΔt \n\n
        (V - V₀) / Δt = a \n\n
        Substitute into \n
        x = x₀ + V₀Δt + ½aΔt² \n\n
        x = x₀ + V₀Δt + ½(V - V₀)Δt² / Δt \n\n
